{
  "expo": {
    "name": "Playroll",
    "slug": "playroll",
    "privacy": "unlisted",
    "sdkVersion": "32.0.0",
    "scheme": "playroll",
    "platforms": ["ios", "android"],
    "version": "1.0.3",
    "orientation": "portrait",
    "icon": "./assets/playroll_icon.png",
    "splash": {
      "image": "./assets/loading.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
